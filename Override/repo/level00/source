int main(int argc, const char **argv)
{
  int result;
  int v4;

  puts("***********************************");
  puts("* \t     -Level00 -\t\t  *");
  puts("***********************************");
  printf("Password:");
  scanf("%d", &v4);
  if ( v4 == 5276 )
  {
    puts("\nAuthenticated!");
    system("/bin/sh");
    result = 0;
  }
  else
  {
    puts("\nInvalid Password!");
    result = 1;
  }
  return result;
}